{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-key": 0,
    "react/no-deprecated": 0,
    "react/prop-types": 0,
    "no-unused-vars": 0,
    "jsx-a11y/anchor-has-content": 0,
    "react/display-name": 0
  },

  "ignorePatterns": ["src/**/*.css", "src/**/*.png", "src/**/*.jpg"]
}
